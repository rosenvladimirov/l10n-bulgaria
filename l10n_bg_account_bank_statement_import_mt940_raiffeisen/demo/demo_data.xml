<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="mt940_rfbb_company_bank" model="res.partner.bank">
            <field name="acc_type">bank</field>
            <field name="acc_number">BG56BRDE360SV52474653600</field>
            <field name="partner_id" ref="base.main_partner"></field>
            <field name="bank_id" ref="base.res_bank_1"/>
        </record>
        
        <record id="rfbb_account_id" model="account.account">
            <field name="code">1020</field>
            <field name="name">RFBB Account</field>
            <field name="reconcile" eval='True'/>
            <field name="user_type_id" ref="account.data_account_type_liquidity"/>
            <field name="currency_id" ref="base.EUR"/>
        </record>
        
        <record id="seq_rfbb_account_journal" model="ir.sequence">
            <field name="name">RFBB940</field>
            <field name="code">account.journal</field>
            <field name="prefix">RFBB940/</field>
            <field name="padding">5</field>
        </record>
        
        <record id="mt940_rfbb_bank_journal" model="account.journal">
            <field name="name">Bank Journal - (test mt940 RFBB)</field>
            <field name="code">RFBB9401</field>
            <field name="type">bank</field>
            <field name="sequence_id" ref="seq_rfbb_account_journal"/>
            <field name="default_debit_account_id" ref="rfbb_account_id"/>
            <field name="default_credit_account_id" ref="rfbb_account_id"/>
            <field name="bank_account_id" ref="mt940_rfbb_company_bank"/>
            <field name="currency_id" ref="base.EUR"/>
        </record>
    </data>
</odoo>
